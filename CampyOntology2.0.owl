                                                                                            rdf:first "i" ;
                                                                                            rdf:rest [ rdf:type rdf:List ;
                                                                                                       rdf:first "intermediate" ;
                                                                                                       rdf:rest [ rdf:type rdf:List ;
                                                                                                                  rdf:first "r" ;
                                                                                                                  rdf:rest [ rdf:type rdf:List ;
                                                                                                                             rdf:first "resistant" ;
                                                                                                                             rdf:rest [ rdf:type rdf:List ;
                                                                                                                                        rdf:first "s" ;
                                                                                                                                        rdf:rest [ rdf:type rdf:List ;
                                                                                                                                                   rdf:first "sensitive" ;
                                                                                                                                                   rdf:rest rdf:nil
                                                                                                                                                 ]
                                                                                                                                      ]
                                                                                                                           ]
                                                                                                                ]
                                                                                                     ]
                                                                                          ]
                                                                               ]
                                                                    ]
                                                         ]
                                             ]
                         ] ,
